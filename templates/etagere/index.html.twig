{% extends 'base.html.twig' %}

{% block title %}Etagere index{% endblock %}

{% block body %}
    <h1>Etagere index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Numero</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for etagere in etageres %}
            <tr>
                <td>{{ etagere.id }}</td>
                <td>{{ etagere.numero }}</td>
                <td>
                    <a href="{{ path('etagere_show', {'id': etagere.id}) }}">show</a>
                    <a href="{{ path('etagere_edit', {'id': etagere.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('etagere_new') }}">Create new</a>
{% endblock %}
