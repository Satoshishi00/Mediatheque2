{% extends 'base.html.twig' %}

{% block body %}
    {{ form_start(form) }}
    {{ form_widget(form) }}
    <button type="submit">Rechercher</button>
    {{ form_end(form) }}

    <h2>Resultats</h2>

    {% if results|length>0 %}
        <ul>
            {% for historique in results %}
                <li>{{ historique }}</li>
            {% endfor %}
        </ul>

    {% else %}No results{% endif %}
{% endblock %}